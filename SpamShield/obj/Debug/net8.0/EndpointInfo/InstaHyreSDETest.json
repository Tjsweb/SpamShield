  "components": {
    "schemas": {
      "ContactModel": {
        "type": "object",
        "properties": {
          "selfPhoneNumber": {
            "type": "string",
            "nullable": true
          },
          "personalContacts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GlobalContactModel"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GlobalContactModel": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "isSpam": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "LoginModel": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterModel": {
        "required": [
          "name",
          "password",
          "phoneNumber"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 8,
            "type": "string",
            "format": "password"
          },
          "confirmPassword": {
            "type": "string",
            "format": "password",
            "nullable": true
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Enter 'Bearer' followed by a space and your JWT token",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
